"EyeRefract"
{
	"$Iris"               "models\omniman\eyes"
	"$AmbientOcclTexture" "models\omniman\eye_ao"
	"$Envmap"             "models\omniman\eye-reflection-cubemap-"   
	"$CorneaTexture"      "models\omniman\eye-cornea"      			

	"$EyeballRadius" "0.5"				// Default 0.5
//	"$AmbientOcclColor" "[0.33 0.33 0.33]"	// Default 0.33, 0.33, 0.33
